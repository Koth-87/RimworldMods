			<Beauty>5</Beauty>
			<WorkToBuild>900</WorkToBuild>
			<Flammability>0.15</Flammability>
			<StyleDominance>1</StyleDominance>
		</statBases>
		<canGenerateDefaultDesignator>false</canGenerateDefaultDesignator>
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
	</StuffedFloors.FloorTypeDef>
	<StuffedFloors.FloorTypeDef MayRequire="Ludeon.RimWorld.Ideology" Name="StuffedFloorsStoneIdeologyBase" ParentName="StuffedFloorsStoneRoyalBase" Abstract="True">
