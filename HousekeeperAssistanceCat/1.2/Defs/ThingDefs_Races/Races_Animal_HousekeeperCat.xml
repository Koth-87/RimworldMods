<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="AnimalThingBase">
		<defName>HousekeeperCat</defName>
		<label>housekeeper cat</label>
		<description>A highly intelligent creature, engineered to be a perfect house pet. It has very dextrous, hand-like paws and can be trained to provide some help with housekeeping.</description>
		<statBases>
			<MoveSpeed>6.00</MoveSpeed>
			<ComfyTemperatureMin>-50</ComfyTemperatureMin>
			<ComfyTemperatureMax>60</ComfyTemperatureMax>
			<MarketValue>1500</MarketValue>
			<MeatAmount>50</MeatAmount>
		</statBases>
		<tools>
			<li>
				<label>left claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>7</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>right claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>7</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>teeth</label>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>10</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>5</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		<comps>
			<li Class="CompProperties_Shearable">
				<woolDef>Wool_HKCat</woolDef>
				<shearIntervalDays>30</shearIntervalDays>
				<woolAmount>20</woolAmount>
			</li>
		</comps>
		<race>
			<thinkTreeMain>Animal</thinkTreeMain>
			<body>Monkey</body>
			<predator>false</predator>
			<packAnimal>true</packAnimal>
			<petness>1</petness>
			<baseBodySize>0.8</baseBodySize>
			<baseHungerRate>0.1</baseHungerRate>
			<baseHealthScale>2</baseHealthScale>
			<foodType>OmnivoreAnimal, OvivoreAnimal, VegetarianRoughAnimal</foodType>
			<leatherDef>Leather_HKCat</leatherDef>
			<gestationPeriodDays>90</gestationPeriodDays>
			<nameOnTameChance>1</nameOnTameChance>
			<trainability>Advanced</trainability>
			<wildness>0</wildness>
			<wildBiomes>
				<SeaIce>0.1</SeaIce>
				<IceSheet>0.1</IceSheet>
				<Tundra>0.05</Tundra>
				<BorealForest>0.1</BorealForest>
				<ColdBog>0.1</ColdBog>
				<TemperateForest>0.05</TemperateForest>
				<TemperateSwamp>0.075</TemperateSwamp>
				<TropicalRainforest>0.05</TropicalRainforest>
				<TropicalSwamp>0.05</TropicalSwamp>
				<AridShrubland>0.075</AridShrubland>
				<Desert>0.1</Desert>
				<ExtremeDesert>0.1</ExtremeDesert>
			</wildBiomes>
			<nuzzleMtbHours>12</nuzzleMtbHours>
			<manhunterOnDamageChance>0.35</manhunterOnDamageChance>
			<manhunterOnTameFailChance>0</manhunterOnTameFailChance>
			<lifeExpectancy>80</lifeExpectancy>
			<useMeatFrom>Cat</useMeatFrom>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.5</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>6</minAge>
					<soundWounded>Pawn_Cat_Injured</soundWounded>
					<soundDeath>Pawn_Cat_Death</soundDeath>
					<soundCall>Pawn_Cat_Call</soundCall>
					<soundAngry>Pawn_Cat_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
			<ageGenerationCurve>
				<points>
					<li>(1,0)</li>
					<li>(7,20)</li>
					<li>(15,60)</li>
					<li>(20,20)</li>
					<li>(25,0)</li>
				</points>
			</ageGenerationCurve>
		</race>
		<tradeTags>
			<li>AnimalUncommon</li>
			<li>AnimalPet</li>
		</tradeTags>
	</ThingDef>
